{
  "name": "govuk-frontend-ui",
  "private": false,
  "version": "0.0.25",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "storybook dev -p 6006",
    "gen-components": "node ./src/scripts/gen-components.js",
    "gen-stories": "node ./src/scripts/gen-stories.js",
    "build": "npm run build-sass && storybook build",
    "build-ts": "tsc",
    "build-sass": "sass ./src/scss:.storybook",
    "build-lib": "rollup -c",
    "patch": "npm version patch",
    "push": "git push",
    "publish-npm": "npm publish",
    "smash": "npm-run-all build-lib patch push publish-npm"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@storybook/addon-actions": "7.0.0-alpha.51",
    "@storybook/addon-docs": "7.0.0-alpha.51",
    "@storybook/addon-essentials": "7.0.0-alpha.51",
    "@storybook/addon-interactions": "7.0.0-alpha.51",
    "@storybook/addon-links": "7.0.0-alpha.51",
    "@storybook/react": "7.0.0-alpha.51",
    "@storybook/react-vite": "7.0.0-alpha.51",
    "@storybook/testing-library": "^0.0.14-next.0",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "babel-loader": "^8.3.0",
    "deep-iterator": "^1.1.0",
    "govuk-frontend": "^4.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-html-parser": "^2.0.2",
    "rollup": "^3.4.0",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "sass": "^1.56.1",
    "storybook": "7.0.0-alpha.51",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "govuk-frontend": "^4.4.0",
    "react": "^18.2.0"
  }
}
